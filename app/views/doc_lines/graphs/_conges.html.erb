<% last_doc = my_doclines.select{|line| line.category == 'conge_n' || line.category == 'conge_n_1' && line.data_entry_period >= start_at && line.data_entry_period <= end_at}.last(2) %>

<%= column_chart [
        ["N-1", last_doc.pluck(:conge_n, :amount)[0][1]],
        ["N", last_doc.pluck(:conge_n_1, :amount)[1][1]]
      ],
      height: "200px",
      colors: ["#4D54E1", "#211d43"],
      library: {
        tooltip: {
          pointFormat: 'Awesome values: <b></b>'
        },
        animation: {
          easing: 'easeOutQuad'
        }
      },
      label: 'Jours restants', defer: true
 %>
